    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.2",
    "css-loader": "^5.2.4",
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^1.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.36.1",
