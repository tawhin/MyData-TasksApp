{
  "name": "mytasks",
  "version": "1.0.0",
  "description": "simple tasks management application",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "start:dev": "concurrently \"npm run dev:server\" \"npm run dev:bundler\"",
    "dev:server": "nodemon --exec babel-node src/server/server.js --ignore dist/",
    "dev:bundler": "webpack -w --mode=development",
    "prod:server": "node --exec babel-node src/server/server.js",
    "prod:bundler": "webpack -w --mode=production"
